<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="count" format="integer" />
    <attr name="speed" format="float" />
    <attr name="max" format="float" />
    <attr name="min" format="float" />
    <attr name="column" format="integer" />
    <attr name="scale" format="float" />
    <attr name="degree" format="integer" />
    <attr name="transX" format="dimension" />
    <attr name="transY" format="dimension" />

    <declare-styleable name="RainView">

        <attr name="count" />
        <attr name="speed" />
        <attr name="max" />
        <attr name="min" />
        <attr name="meteor" format="reference" />

    </declare-styleable>

    <declare-styleable name="InflaterFactory">

        <attr name="android:id" />

    </declare-styleable>

    <declare-styleable name="AutomaticLayout">

        <attr name="orientation">
            <flag name="horizontal" value="0" />
            <flag name="vertical" value="1" />
        </attr>

        <attr name="column" />

    </declare-styleable>

    <declare-styleable name="ImageView">

        <attr name="scale" />
        <attr name="image_shape">
            <flag name="circle" value="1" />
            <flag name="rectangle" value="0" />
        </attr>

    </declare-styleable>

    <declare-styleable name="RotateTextView">

        <attr name="degree" />
        <attr name="transX" />
        <attr name="transY" />

    </declare-styleable>

</resources>